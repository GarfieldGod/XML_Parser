{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "ZhmlParser Debug",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "${cwd}",
      //"program": "${workspaceFolder}/build/Debug/${fileBasenameNoExtension}.exe",
      "program": "${workspaceFolder}/build/Debug/zhml.exe",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "preLaunchTask": "ZhmlParser Build",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}